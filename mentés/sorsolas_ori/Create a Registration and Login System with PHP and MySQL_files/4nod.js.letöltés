(function($){
"use strict";
$(document).ready(function(){
if($('.toggle-menu').length){
$('.toggle-menu').click(function(){
$('#nav-wrapper .primary-menu').toggle();
});
}
$('.primary-menu .caret').click(function(){
var $submenu=$(this).closest('.menu-item-has-children').find(' > .sub-menu');
$submenu.toggle();
return false;
});
$(".nav-toggle").on("click", function(){
$(this).toggleClass("active");
$(".primary-menu").slideToggle();
});
$(document).on('click', '.social-share.share-buttons a[target="_blank"], a.social-icon[target="_blank"]', function(event){
event.preventDefault();
var window_size='';
var url=this.href;
var domain=url.split("/")[2];
switch(domain){
case "www.facebook.com":
window_size="width=585,height=368";
break;
case "twitter.com":
window_size="width=585,height=261";
break;
case "linedin.com":
window_size="width=517,height=511";
break;
case "pinterest.com":
window_size="width=850,height=550";
break;
case "getpocket.com":
window_size="width=850,height=550";
break;
default:
window_size="width=585,height=515";
}
window.open(url, '', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,' + window_size);
});
$(document).ready(function(){
$(".container").fitVids();
});
if($('.post').length){
$('.post').fitVids();
}
setTimeout(function(){
var $sticky_el=$(".make_sticky").closest('.ai_widget');
if($sticky_el.length > 0){
$sticky_el.scrollToFixed({
minWidth: 990,
marginTop: 0,
limit: function (){
var limit=$('footer').offset().top - $sticky_el.outerHeight(true) - 285;
return limit;
}});
}}, 5000);
